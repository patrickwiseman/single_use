<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.a1 %></td>
    <td><%= user.a2 %></td>
    <td><%= user.a3 %></td>
    <td><%= user.a4 %></td>
    <td><%= user.a5 %></td>
    <td><%= user.a6 %></td>
    <td><%= user.a7 %></td>
    <td><%= user.a8 %></td>
    <td><%= user.a9 %></td>
    <td><%= user.a10 %></td>
    <td><%= user.a11 %></td>
    <td><%= user.a12 %></td>
    <td><%= user.a13 %></td>
    <td><%= user.a14 %></td>
    <td><%= user.a15 %></td>
    <td><%= user.a16 %></td>
    <td><%= user.a17 %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

